      RowBox[{"{", 
       RowBox[{"0", ",", 
        RowBox[{"mass", "*", "g"}]}], "}"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "because", " ", "the", " ", "projectile", " ", "is", " ", "in", " ", 
       "freefall"}], "*)"}], " ", "\[IndentingNewLine]", 
     RowBox[{"acc", "=", 
      RowBox[{"force", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vel", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"vel", "/.", 
         RowBox[{"t", "->", "timeThrustApplied"}]}], ")"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"acc", ",", "t"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"posStageTwo", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"posStageOne", "/.", 
         RowBox[{"t", "->", "timeThrustApplied"}]}], ")"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"vel", ",", "t"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "calculate", " ", "the", " ", "time", " ", "it", " ", "will", " ", 
        "take", " ", "the", " ", "projectile", " ", "to", " ", "reach", " ", 
        "the", " ", "ground"}], ",", " ", 
       RowBox[{
       "where", " ", "the", " ", "y", " ", "component", " ", "of", " ", 
        "position", " ", "is", " ", "less", " ", "than", " ", "0"}]}], "*)"}],
      "\[IndentingNewLine]", 
     RowBox[{"timeToGround", "=", 
      RowBox[{"t", "/.", 
       RowBox[{"FindRoot", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"posStageTwo", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "==", "0"}], ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "100"}], "}"}]}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Current", " ", 
        RowBox[{"issue", ":", " ", 
         RowBox[{
         "how", " ", "can", " ", "we", " ", "reliably", " ", "find", " ", 
          "the", " ", "positive", " ", "root"}]}]}], ",", " ", 
       RowBox[{"not", " ", "the", " ", "negative", " ", 
        RowBox[{"one", "?"}]}]}], "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"set", " ", "up", " ", "the", " ", "parametric", " ", "plot"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"freefallPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{"posStageTwo", ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeToGround"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Red", ",", " ", "Dashed"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
