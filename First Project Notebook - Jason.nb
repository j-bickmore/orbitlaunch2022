Cell[BoxData[
 RowBox[{
  RowBox[{"twoStage", "[", 
   RowBox[{
   "thrustx_", ",", "thrusty_", ",", "timeThrustApplied_", ",", "mass_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "g", ",", "force", ",", "acc", ",", "vel", ",", "pos", ",", "t", ",", 
      "timeToGround", ",", "thrustPlot", ",", "freefallPlot"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"define", " ", "g"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"g", "=", 
      RowBox[{"-", "9.8"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"thrust", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"force", "=", 
      RowBox[{"{", 
       RowBox[{"thrustx", ",", 
        RowBox[{"thrusty", "+", 
         RowBox[{"mass", "*", "g"}]}]}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"acc", "=", 
      RowBox[{"force", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vel", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"acc", ",", "t"}], "]"}]}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"No", " ", "initial", " ", "velocity"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"pos", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"vel", ",", "t"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "set", " ", "up", " ", "the", " ", "parametric", " ", "plot", " ", 
       "for", " ", "thrust", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"thrustPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{"pos", ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeThrustApplied"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Blue", ",", " ", "Dashed"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"freefall", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"force", "=", 
      RowBox[{"{", 
       RowBox[{"0", ",", 
        RowBox[{"mass", "*", "g"}]}], "}"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "because", " ", "the", " ", "projectile", " ", "is", " ", "in", " ", 
       "freefall"}], "*)"}], " ", "\[IndentingNewLine]", 
     RowBox[{"acc", "=", 
      RowBox[{"force", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vel", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"vel", "/.", 
         RowBox[{"t", "->", "timeThrustApplied"}]}], ")"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"acc", ",", "t"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"pos", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"pos", "/.", 
         RowBox[{"t", "->", "timeThrustApplied"}]}], ")"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"vel", ",", "t"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "calculate", " ", "the", " ", "time", " ", "it", " ", "will", " ", 
        "take", " ", "the", " ", "projectile", " ", "to", " ", "reach", " ", 
        "the", " ", "ground"}], ",", " ", 
       RowBox[{
       "where", " ", "the", " ", "y", " ", "component", " ", "of", " ", 
        "position", " ", "is", " ", "less", " ", "than", " ", "0"}]}], "*)"}],
      "\[IndentingNewLine]", 
     RowBox[{"timeToGround", "=", 
      RowBox[{"t", "/.", 
       RowBox[{"FindRoot", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"pos", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "==", "0"}], ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "100"}], "}"}]}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Current", " ", 
        RowBox[{"issue", ":", " ", 
         RowBox[{
         "how", " ", "can", " ", "we", " ", "reliably", " ", "find", " ", 
          "the", " ", "positive", " ", "root"}]}]}], ",", " ", 
       RowBox[{"not", " ", "the", " ", "negative", " ", 
        RowBox[{"one", "?"}]}]}], "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"set", " ", "up", " ", "the", " ", "parametric", " ", "plot"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"freefallPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{"pos", ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeToGround"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Red", ",", " ", "Dashed"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Show", " ", "the", " ", "final", " ", "plot"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Show", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"thrustPlot", ",", "freefallPlot"}], "}"}], ",", 
       RowBox[{"PlotRange", "->", "All"}], ",", 
       RowBox[{"AxesLabel", "->", 
        RowBox[{"{", 
         RowBox[{"\"\<x Position\>\"", ",", "\"\<y Position\>\""}], "}"}]}], 
       ",", 
       RowBox[{"PlotLabel", "->", 
        RowBox[{"StringForm", "[", 
         RowBox[{"\"\<Time to ground: `` seconds\>\"", ",", 
          RowBox[{"timeThrustApplied", "+", "timeToGround"}]}], "]"}]}]}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.8781477521741037`*^9, 3.8781477541096277`*^9}, {
