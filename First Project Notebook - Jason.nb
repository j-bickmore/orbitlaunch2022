Cell[BoxData[
 RowBox[{
  RowBox[{"twoStage", "[", 
   RowBox[{
   "thrustx_", ",", "thrusty_", ",", "timeThrustApplied_", ",", "mass_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "g", ",", "forceStageOne", ",", "accStageOne", ",", "velStageOne", ",", 
      "posStageOne", ",", "forceStageTwo", ",", "accStageTwo", ",", 
      "velStageTwo", ",", "posStageTwo", ",", "t", ",", "timeToGround", ",", 
      "thrustPlot", ",", "freefallPlot"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"define", " ", "g"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"g", "=", 
      RowBox[{"-", "9.8"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"thrust", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"forceStageOne", "=", 
      RowBox[{"{", 
       RowBox[{"thrustx", ",", 
        RowBox[{"thrusty", "+", 
         RowBox[{"mass", "*", "g"}]}]}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"accStageOne", "=", 
      RowBox[{"forceStageOne", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"velStageOne", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"accStageOne", ",", "t"}], "]"}]}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"No", " ", "initial", " ", "velocity"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"posStageOne", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"velStageOne", ",", "t"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"set", " ", "up", " ", "the", " ", "parametric", " ", "plot"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"thrustPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{"posStageOne", ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeThrustApplied"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Blue", ",", " ", "Dashed"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"freefall", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"forceStageTwo", "=", 
      RowBox[{"{", 
       RowBox[{"0", ",", 
        RowBox[{"mass", "*", "g"}]}], "}"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "because", " ", "the", " ", "projectile", " ", "is", " ", "in", " ", 
       "freefall"}], "*)"}], " ", "\[IndentingNewLine]", 
     RowBox[{"accStageTwo", "=", 
      RowBox[{"forceStageTwo", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"velStageTwo", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"velStageOne", "/.", 
         RowBox[{"t", "->", "timeThrustApplied"}]}], ")"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"accStageTwo", ",", "t"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"posStageTwo", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"posStageOne", "/.", 
         RowBox[{"t", "->", "timeThrustApplied"}]}], ")"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"accStageTwo", ",", "t"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "calculate", " ", "the", " ", "time", " ", "it", " ", "will", " ", 
        "take", " ", "the", " ", "projectile", " ", "to", " ", "reach", " ", 
        "the", " ", "ground"}], ",", " ", 
       RowBox[{
       "where", " ", "the", " ", "y", " ", "component", " ", "of", " ", 
        "position", " ", "is", " ", "less", " ", "than", " ", "0"}]}], "*)"}],
      "\[IndentingNewLine]", 
     RowBox[{"timeToGround", "=", 
      RowBox[{"t", "/.", 
       RowBox[{"FindRoot", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"posStageTwo", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "==", "0"}], ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "100"}], "}"}]}], "]"}]}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Current", " ", 
        RowBox[{"issue", ":", " ", 
         RowBox[{
         "how", " ", "can", " ", "we", " ", "reliably", " ", "find", " ", 
          "the", " ", "positive", " ", "root"}]}]}], ",", " ", 
       RowBox[{"not", " ", "the", " ", "negative", " ", 
        RowBox[{"one", "?"}]}]}], "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"set", " ", "up", " ", "the", " ", "parametric", " ", "plot"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"freefallPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{"posStageTwo", ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeToGround"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Red", ",", " ", "Dashed"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Show", " ", "the", " ", "final", " ", "plot"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Show", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"thrustPlot", ",", "freefallPlot"}], "}"}], ",", 
       RowBox[{"PlotRange", "->", "All"}], ",", 
       RowBox[{"AxesLabel", "->", 
        RowBox[{"{", 
         RowBox[{"\"\<x Position\>\"", ",", "\"\<y Position\>\""}], "}"}]}], 
       ",", 
       RowBox[{"PlotLabel", "->", 
        RowBox[{"StringForm", "[", 
         RowBox[{"\"\<Time to ground: `` seconds\>\"", ",", 
          RowBox[{"timeThrustApplied", "+", "timeToGround"}]}], "]"}]}]}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
