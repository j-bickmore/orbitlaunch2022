     RowBox[{"(*", 
      RowBox[{"forceGravitation", ":="}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"force", "=", 
      RowBox[{"Piecewise", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
