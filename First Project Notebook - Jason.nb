Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"TwoStageOrbit", "[", 
    RowBox[{
    "thrustx_", ",", "thrusty_", ",", "timeThrustApplied_", ",", "mass_"}], 
    "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "g", ",", "massEarth", ",", "radiusEarth", ",", "centerOfEarth", ",", 
       "bigG", ",", "maxAirTime", ",", "r", ",", "forceGravitation", ",", 
       "force", ",", "acc", ",", "vel", ",", "pos", ",", "t", ",", 
       "closestToEarth", ",", "timeClosestToEarth", ",", "timeToGround", ",", 
       "thrustPlot", ",", "freefallPlot", ",", "earthObject"}], "}"}], ",", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"define", " ", "constants"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"g", "=", 
       RowBox[{"-", "9.8"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"massEarth", "=", 
       RowBox[{"16900", "*", "9.8"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"radiusEarth", "=", "130"}], ";", "\[IndentingNewLine]", 
      RowBox[{"centerOfEarth", "=", 
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"bigG", "=", "1"}], ";", "\[IndentingNewLine]", 
      RowBox[{"maxAirTime", "=", "20"}], ";", " ", 
      RowBox[{"(*", 
       RowBox[{
       "maxAirTime", " ", "is", " ", "maximum", " ", "time", " ", "we", " ", 
        "will", " ", "let", " ", "a", " ", "projectile", " ", "remain", " ", 
        "in", " ", "the", " ", "air", " ", "before", " ", "we", " ", 
        "declare", " ", "it", " ", "as", " ", "\"\<in orbit or lost\>\""}], 
       "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "set", " ", "up", " ", "expressions", " ", "related", " ", "to", " ", 
        "universal", " ", "gravitation", " ", "initially"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"r", "=", 
       RowBox[{"centerOfEarth", "-", 
        RowBox[{"{", 
         RowBox[{"0", ",", "radiusEarth"}], "}"}]}]}], ";", " ", 
      RowBox[{"(*", 
       RowBox[{
       "r", " ", "is", " ", "a", " ", "vector", " ", "from", " ", "the", " ", 
        "projectile", " ", "to", " ", "the", " ", "center", " ", "of", " ", 
        "the", " ", 
        RowBox[{"earth", ".", " ", "This"}], " ", "is", " ", "based", " ", 
        "on", " ", 
        StyleBox["initial",
         FontWeight->"Bold"], " ", "position"}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"forceGravitation", "=", 
       RowBox[{"r", "*", 
        RowBox[{"(", 
         RowBox[{"bigG", "*", "mass", "*", 
          RowBox[{"massEarth", "/", 
           RowBox[{
            RowBox[{"Norm", "[", "r", "]"}], "^", "3"}]}]}], ")"}]}]}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "initial", " ", "force", " ", "of", " ", "gravitation", " ", "at", " ",
         "launch"}], "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"thrust", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"force", "=", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"thrustx", ",", "thrusty"}], "}"}], "+", 
        "forceGravitation"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"acc", "=", 
       RowBox[{"force", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"vel", "=", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", "0"}], "}"}], "+", 
        RowBox[{"Integrate", "[", 
         RowBox[{"acc", ",", "t"}], "]"}]}]}], ";", " ", 
      RowBox[{"(*", 
