Cell[BoxData[
 RowBox[{
  RowBox[{"twoStage", "[", 
   RowBox[{
   "thrustx_", ",", "thrusty_", ",", "timeThrustApplied_", ",", "mass_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"g", "=", 
       RowBox[{"-", "9.8"}]}], ",", "force", ",", "acc", ",", "vel", ",", 
      "posThrustStage", ",", "posFreefallStage", ",", "aircraft", ",", "t", 
      ",", "timeToGround", ",", "thrustPlot", ",", "freefallPlot"}], "}"}], 
    ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"thrust", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"force", "=", 
      RowBox[{"{", 
       RowBox[{"thrustx", ",", 
        RowBox[{"thrusty", "+", 
         RowBox[{"mass", "*", "g"}]}]}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"acc", "=", 
      RowBox[{"force", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"vel", "[", "t_", "]"}], "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"acc", ",", "t"}], "]"}]}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"No", " ", "initial", " ", "velocity"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"posThrustStage", "[", "t_", "]"}], "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{"vel", "[", "t", "]"}], ",", "t"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "set", " ", "up", " ", "the", " ", "parametric", " ", "plot", " ", 
       "for", " ", "thrust", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"thrustPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{
        RowBox[{"posThrustStage", "[", "t", "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeThrustApplied"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Blue", ",", " ", "Dashed"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"freefall", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"force", "=", 
      RowBox[{"{", 
       RowBox[{"0", ",", 
        RowBox[{"mass", "*", "g"}]}], "}"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "because", " ", "the", " ", "projectile", " ", "is", " ", "in", " ", 
       "freefall"}], "*)"}], " ", "\[IndentingNewLine]", 
     RowBox[{"acc", "=", 
      RowBox[{"force", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"vel", "[", "t_", "]"}], "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"vel", "[", "timeThrustApplied", "]"}], ")"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"acc", ",", "t"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"posFreefallStage", "[", "t_", "]"}], "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"posThrustStage", "[", "timeThrustApplied", "]"}], ")"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{"vel", "[", "t", "]"}], ",", "t"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "calculate", " ", "the", " ", "time", " ", "it", " ", "will", " ", 
        "take", " ", "the", " ", "projectile", " ", "to", " ", "reach", " ", 
        "the", " ", "ground"}], ",", " ", 
       RowBox[{
       "where", " ", "the", " ", "y", " ", "component", " ", "of", " ", 
        "position", " ", "is", " ", "less", " ", "than", " ", "0"}]}], "*)"}],
      "\[IndentingNewLine]", 
     RowBox[{"timeToGround", "=", 
      RowBox[{"t", "/.", 
       RowBox[{"FindRoot", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"posFreefallStage", "[", "t", "]"}], "[", 
           RowBox[{"[", "2", "]"}], "]"}], "==", "0"}], ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "100"}], "}"}]}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Current", " ", 
        RowBox[{"issue", ":", " ", 
         RowBox[{
         "how", " ", "can", " ", "we", " ", "reliably", " ", "find", " ", 
          "the", " ", "positive", " ", "root"}]}]}], ",", " ", 
       RowBox[{"not", " ", "the", " ", "negative", " ", 
        RowBox[{"one", "?"}]}]}], "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "set", " ", "up", " ", "module", " ", "to", " ", "switch", " ", "which",
        " ", "position", " ", "function", " ", "is", " ", "used", " ", 
       "based", " ", "on", " ", "t"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"aircraft", "[", "t_", "]"}], ":=", 
      RowBox[{"Module", "[", 
       RowBox[{
        RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"t", "<=", "timeThrustApplied"}], ",", 
          RowBox[{"posThrustStage", "[", "t", "]"}], ",", 
          RowBox[{"posFreefallStage", "[", 
           RowBox[{"t", "-", "timeThrustApplied"}], "]"}]}], "]"}]}], "]"}]}],
      ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"set", " ", "up", " ", "the", " ", "parametric", " ", "plot"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"freefallPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{
        RowBox[{"posFreefallStage", "[", "t", "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeToGround"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Red", ",", " ", "Dashed"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Show", " ", "the", " ", "final", " ", "plot"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Show", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"thrustPlot", ",", "freefallPlot"}], "}"}], ",", 
        RowBox[{"PlotRange", "->", "All"}], ",", 
        RowBox[{"AxesLabel", "->", 
         RowBox[{"{", 
          RowBox[{"\"\<x Position\>\"", ",", "\"\<y Position\>\""}], "}"}]}], 
        ",", 
        RowBox[{"PlotLabel", "->", 
         RowBox[{"StringForm", "[", 
          RowBox[{"\"\<Time to ground: `` seconds\>\"", ",", 
           RowBox[{"timeThrustApplied", "+", "timeToGround"}]}], "]"}]}]}], 
       "]"}], "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"show", " ", "the", " ", "animation"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Animate", "[", 
      RowBox[{
       RowBox[{"Show", "[", 
        RowBox[{"thrustPlot", ",", "freefallPlot", ",", 
         RowBox[{"Graphics", "[", 
          RowBox[{"Point", "[", 
           RowBox[{"aircraft", "[", "t", "]"}], "]"}], "]"}], ",", 
         RowBox[{"PlotRange", "->", "All"}], ",", 
         RowBox[{"AxesLabel", "->", 
          RowBox[{"{", 
           RowBox[{"\"\<x Position\>\"", ",", "\"\<y Position\>\""}], "}"}]}],
          ",", 
         RowBox[{"PlotLabel", "->", 
          RowBox[{"StringForm", "[", 
           RowBox[{"\"\<Time to ground: `` seconds\>\"", ",", 
            RowBox[{"timeThrustApplied", "+", "timeToGround"}]}], "]"}]}]}], 
        "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"t", ",", "0", ",", 
         RowBox[{"timeThrustApplied", "+", "timeToGround"}]}], "}"}]}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
