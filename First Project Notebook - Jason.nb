Cell[BoxData[{
 RowBox[{"force", "=", 
  RowBox[{"Piecewise", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"2", ",", "2"}], "}"}], ",", 
       RowBox[{"t", "<", "5"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", 
         RowBox[{"-", "9.8"}]}], "}"}], ",", 
       RowBox[{"t", ">=", "5"}]}], "}"}]}], "}"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"acceleration", "=", 
  RowBox[{"force", "/", "2"}]}], "\[IndentingNewLine]", 
 RowBox[{"velocity", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{"acceleration", ",", "t"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"position", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{"velocity", ",", "t"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"ParametricPlot", "[", 
  RowBox[{"position", ",", 
