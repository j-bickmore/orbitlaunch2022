   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "g", ",", "force", ",", "acc", ",", "vel", ",", "posStageOne", ",", 
      "posStageTwo", ",", "t", ",", "timeToGround", ",", "thrustPlot", ",", 
      "freefallPlot"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"define", " ", "g"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"g", "=", 
      RowBox[{"-", "9.8"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"thrust", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"force", "=", 
      RowBox[{"{", 
       RowBox[{"thrustx", ",", 
        RowBox[{"thrusty", "+", 
         RowBox[{"mass", "*", "g"}]}]}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"acc", "=", 
      RowBox[{"force", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vel", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"acc", ",", "t"}], "]"}]}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"No", " ", "initial", " ", "velocity"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"posStageOne", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"vel", ",", "t"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "set", " ", "up", " ", "the", " ", "parametric", " ", "plot", " ", 
       "for", " ", "thrust", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"thrustPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{"posStageOne", ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeThrustApplied"}], "}"}], ",", 
