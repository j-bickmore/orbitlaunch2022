Cell[CellGroupData[{

Cell["Second Milestone", "Subsection",
 CellChangeTimes->{{3.877631026757095*^9, 
  3.8776310316897635`*^9}},ExpressionUUID->"fd45afbd-6b8a-4bde-8c53-\
5305dce47526"],

Cell["\<\
The second major milestone will be to create a 2D graph of orbital motion \
around the earth. 

Two objects will be animated, where one is the earth, which wouldn\
\[CloseCurlyQuote]t move. The second would be the rocket\[CloseCurlyQuote]s \
position. For this one, the plot will be on an {x,y} axis with the time \
factor changing. 
\
\>", "Text",
 CellChangeTimes->{
  3.8776310816308484`*^9, {3.8776315478906846`*^9, 3.877631576668162*^9}, {
   3.8776317073524*^9, 3.8776317145002327`*^9}, {3.8776317616324596`*^9, 
   3.877631763160509*^9}, {3.878670381236085*^9, 
   3.878670388655183*^9}},ExpressionUUID->"3a8037e1-a1a0-4a18-9639-\
d253c9272ec5"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"tempPlot", ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"twoStageOrbit", "[", 
   RowBox[{
   "thrustx_", ",", "thrusty_", ",", "timeThrustApplied_", ",", "mass_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "g", ",", "massEarth", ",", "radiusEarth", ",", "centerOfEarth", ",", 
      "bigG", ",", "r", ",", "maxAirTime", ",", "force", ",", "acc", ",", 
      "vel", ",", "pos", ",", "t", ",", "closestToEarth", ",", 
      "timeClosestToEarth", ",", "timeToGround", ",", "thrustPlot", ",", 
      "freefallPlot", ",", "earthObject"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"define", " ", "constants"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"g", "=", 
      RowBox[{"-", "9.8"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"massEarth", "=", "100"}], ";", "\[IndentingNewLine]", 
     RowBox[{"radiusEarth", "=", "70"}], ";", "\[IndentingNewLine]", 
     RowBox[{"centerOfEarth", "=", 
      RowBox[{"{", 
       RowBox[{"0", ",", "0"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"bigG", "=", "10"}], ";", "\[IndentingNewLine]", 
     RowBox[{"r", ":=", 
      RowBox[{"centerOfEarth", "-", "pos"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "r", " ", "is", " ", "a", " ", "vector", " ", "from", " ", "the", " ", 
       "projectile", " ", "to", " ", "the", " ", "center", " ", "of", " ", 
       "the", " ", "earth"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"forceGravitation", ":="}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"maxAirTime", "=", "20"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"thrust", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"force", "=", 
      RowBox[{"{", 
       RowBox[{"thrustx", ",", 
        RowBox[{"thrusty", "+", 
         RowBox[{"mass", "*", "g"}]}]}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"acc", "=", 
      RowBox[{"force", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vel", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"acc", ",", "t"}], "]"}]}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"No", " ", "initial", " ", "velocity"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"pos", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "radiusEarth"}], "}"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"vel", ",", "t"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "set", " ", "up", " ", "the", " ", "parametric", " ", "plot", " ", 
       "for", " ", "thrust", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"thrustPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{"pos", ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeThrustApplied"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Blue", ",", " ", "Dashed"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"freefall", " ", "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"force", "=", 
      RowBox[{"{", 
       RowBox[{"0", ",", 
        RowBox[{"mass", "*", "g"}]}], "}"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "because", " ", "the", " ", "projectile", " ", "is", " ", "in", " ", 
       "freefall"}], "*)"}], " ", "\[IndentingNewLine]", 
     RowBox[{"acc", "=", 
      RowBox[{"force", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vel", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"vel", "/.", 
         RowBox[{"t", "->", "timeThrustApplied"}]}], ")"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"acc", ",", "t"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"pos", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"pos", "/.", 
         RowBox[{"t", "->", "timeThrustApplied"}]}], ")"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"vel", ",", "t"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "calculate", " ", "the", " ", "time", " ", "it", " ", "will", " ", 
       "take", " ", "the", " ", "projectile", " ", "to", " ", "reach", " ", 
       "the", " ", "surface", " ", "of", " ", "the", " ", "Earth"}], "*)"}], 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"first", ",", " ", 
       RowBox[{
       "find", " ", "a", " ", "local", " ", "minimum", " ", "that", " ", 
        "represents", " ", "the", " ", "closest", " ", "the", " ", 
        "projectile", " ", "gets", " ", "to", " ", "the", " ", "earth"}]}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "maxAirTime", " ", "is", " ", "maximum", " ", "time", " ", "we", " ", 
       "will", " ", "let", " ", "a", " ", "projectile", " ", "remain", " ", 
       "in", " ", "the", " ", "air", " ", "before", " ", "we", " ", "declare",
        " ", "it", " ", "as", " ", "\"\<in orbit or lost\>\""}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"closestToEarth", "=", 
      RowBox[{"FindMinimum", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Norm", "[", "r", "]"}], ",", 
          RowBox[{"t", ">", "0"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "maxAirTime"}], "}"}]}], "]"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "finds", " ", "the", " ", "first", " ", "minimum", " ", "value", " ", 
       "of", " ", "r", " ", "where", " ", "t", " ", "is", " ", "between", " ",
        "0", " ", "and", " ", "maxAirtime"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"timeClosestToEarth", "=", 
      RowBox[{"t", "/.", 
       RowBox[{"closestToEarth", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "if", " ", "the", " ", "closest", " ", "the", " ", "projectile", " ", 
        "gets", " ", "is", " ", "less", " ", "than", " ", "the", " ", 
        "radius", " ", "of", " ", "the", " ", "Earth"}], "..."}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"closestToEarth", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "<", "radiusEarth"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"timeToGround", "=", 
        RowBox[{"t", "/.", 
         RowBox[{"FindRoot", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Norm", "[", "r", "]"}], "==", "radiusEarth"}], ",", 
           RowBox[{"{", 
            RowBox[{"t", ",", 
             RowBox[{"timeClosestToEarth", "/", "2"}]}], "}"}]}], "]"}]}]}], 
       ",", " ", 
       RowBox[{"(*", 
        RowBox[{
        "look", " ", "for", " ", "when", " ", "the", " ", "projectile", " ", 
         "hits", " ", "the", " ", 
         RowBox[{"ground", "!"}]}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"timeToGround", "=", "maxAirTime"}]}], " ", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "the", " ", "projectile", " ", "never", " ", "hits", " ", "the", " ", 
         "ground"}], ",", " ", 
        RowBox[{
        "so", " ", "we", " ", "set", " ", "plot", " ", "all", " ", "of", " ", 
         "maxAirTime"}]}], "*)"}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"tempPlot", "=", 
        RowBox[{"Plot", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Norm", "[", "r", "]"}], ",", "radiusEarth"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"t", ",", "0", ",", "10"}], "}"}]}], "]"}]}], ";"}], "*)"}]
      , "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"set", " ", "up", " ", "the", " ", "parametric", " ", "plot"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"freefallPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{"pos", ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeToGround"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Red", ",", " ", "Dashed"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "set", " ", "up", " ", "earth", " ", "graphics", " ", "object"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"earthObject", "=", 
      RowBox[{"Graphics", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Darker", "[", "Green", "]"}], ",", 
         RowBox[{"Disk", "[", 
          RowBox[{"centerOfEarth", ",", "radiusEarth"}], "]"}]}], "}"}], 
       "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Show", " ", "the", " ", "final", " ", "plot"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Show", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"earthObject", ",", "thrustPlot", ",", "freefallPlot"}], 
        "}"}], ",", 
       RowBox[{"Axes", "->", "True"}], ",", 
       RowBox[{"PlotRange", "->", "All"}], ",", 
       RowBox[{"AxesLabel", "->", 
        RowBox[{"{", 
         RowBox[{"\"\<x Position\>\"", ",", "\"\<y Position\>\""}], "}"}]}], 
       ",", 
       RowBox[{"PlotLabel", "->", 
        RowBox[{"StringForm", "[", 
         RowBox[{"\"\<Time to ground: `` seconds\>\"", ",", 
          RowBox[{"timeThrustApplied", "+", "timeToGround"}]}], "]"}]}]}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]}], "Input",
