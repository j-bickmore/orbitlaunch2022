Cell[TextData[StyleBox["Create a version of Orbit using a piecewise function. \
Currently BROKEN",
 FontWeight->"Bold"]], "Text",
 CellChangeTimes->{{3.8791009291542225`*^9, 3.879100950195346*^9}, {
  3.879103530388598*^9, 
  3.879103534656973*^9}},ExpressionUUID->"7178a41d-d64c-453d-9d32-\
fdc78e032972"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Orbit", "[", 
   RowBox[{
   "thrustx_", ",", "thrusty_", ",", "timeThrustApplied_", ",", "mass_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "g", ",", "massEarth", ",", "radiusEarth", ",", "centerOfEarth", ",", 
      "bigG", ",", "r", ",", "maxAirTime", ",", "force", ",", "acc", ",", 
      "vel", ",", "pos", ",", "t", ",", "closestToEarth", ",", 
      "timeClosestToEarth", ",", "timeToGround", ",", "thrustPlot", ",", 
      "freefallPlot", ",", "earthObject"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{"define", " ", "constants"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"g", "=", 
      RowBox[{"-", "9.8"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"massEarth", "=", "100"}], ";", "\[IndentingNewLine]", 
     RowBox[{"radiusEarth", "=", "70"}], ";", "\[IndentingNewLine]", 
     RowBox[{"centerOfEarth", "=", 
      RowBox[{"{", 
       RowBox[{"0", ",", "0"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"bigG", "=", "10"}], ";", "\[IndentingNewLine]", 
     RowBox[{"maxAirTime", "=", "20"}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "maxAirTime", " ", "is", " ", "maximum", " ", "time", " ", "we", " ", 
       "will", " ", "let", " ", "a", " ", "projectile", " ", "remain", " ", 
       "in", " ", "the", " ", "air", " ", "before", " ", "we", " ", "declare",
        " ", "it", " ", "as", " ", "\"\<in orbit or lost\>\""}], "*)"}], 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "set", " ", "up", " ", "force", " ", "as", " ", "a", " ", "piecewise", 
       " ", "function"}], "*)"}], " ", 
     RowBox[{"(*", 
      RowBox[{"forceGravitation", ":="}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"force", "=", 
      RowBox[{"Piecewise", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"thrustx", ",", 
             RowBox[{"thrusty", "+", 
              RowBox[{"mass", "*", "g"}]}]}], "}"}], ",", 
           RowBox[{"t", "<=", "timeThrustApplied"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", 
             RowBox[{"mass", "*", "g"}]}], "}"}], ",", 
           RowBox[{"t", ">", "timeThrustApplied"}]}], "}"}]}], "}"}], "]"}]}],
      ";", "\[IndentingNewLine]", 
     RowBox[{"acc", "=", 
      RowBox[{"force", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vel", "=", 
      RowBox[{"Integrate", "[", 
       RowBox[{"acc", ",", "t"}], "]"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{"No", " ", "initial", " ", "velocity"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"pos", "=", 
      RowBox[{"Integrate", "[", 
       RowBox[{"vel", ",", "t"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"pos", "/.", 
         RowBox[{"t", "->", "2"}]}], ")"}], "+", 
       RowBox[{"{", 
        RowBox[{"1", ",", "0"}], "}"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"temp", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", ",", "0"}], "}"}], "+", "pos"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"temp", "/.", 
       RowBox[{"t", "->", "2"}]}], "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"r", ":=", 
      RowBox[{"centerOfEarth", "-", "pos"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "r", " ", "is", " ", "a", " ", "vector", " ", "from", " ", "the", " ", 
       "projectile", " ", "to", " ", "the", " ", "center", " ", "of", " ", 
       "the", " ", "earth"}], "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"tempPlot", "=", 
      RowBox[{"Plot", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Norm", "[", "r", "]"}], ",", "radiusEarth"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "15"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "calculate", " ", "the", " ", "time", " ", "it", " ", "will", " ", 
       "take", " ", "the", " ", "projectile", " ", "to", " ", "reach", " ", 
       "the", " ", "surface", " ", "of", " ", "the", " ", "Earth"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"first", ",", " ", 
       RowBox[{
       "find", " ", "a", " ", "local", " ", "minimum", " ", "that", " ", 
        "represents", " ", "the", " ", "closest", " ", "the", " ", 
        "projectile", " ", "gets", " ", "to", " ", "the", " ", "center", " ", 
        "of", " ", "the", " ", "earth"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"closestToEarth", "=", 
      RowBox[{"FindMinimum", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Norm", "[", "r", "]"}], ",", 
          RowBox[{"t", ">", "0"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "maxAirTime"}], "}"}]}], "]"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "finds", " ", "the", " ", "first", " ", "minimum", " ", "value", " ", 
       "of", " ", "r", " ", "where", " ", "t", " ", "is", " ", "between", " ",
        "0", " ", "and", " ", "maxAirtime"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"timeClosestToEarth", "=", 
      RowBox[{"t", "/.", 
       RowBox[{"closestToEarth", "[", 
        RowBox[{"[", "2", "]"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "if", " ", "the", " ", "closest", " ", "the", " ", "projectile", " ", 
        "gets", " ", "is", " ", "less", " ", "than", " ", "the", " ", 
        "radius", " ", "of", " ", "the", " ", "Earth"}], "..."}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"closestToEarth", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "<", "radiusEarth"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"timeToGround", "=", 
        RowBox[{"t", "/.", 
         RowBox[{"FindRoot", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Norm", "[", "r", "]"}], "==", "radiusEarth"}], ",", 
           RowBox[{"{", 
            RowBox[{"t", ",", 
             RowBox[{"timeClosestToEarth", "/", "2"}]}], "}"}]}], "]"}]}]}], 
       ",", " ", 
       RowBox[{"(*", 
        RowBox[{
        "look", " ", "for", " ", "when", " ", "the", " ", "projectile", " ", 
         "hits", " ", "the", " ", 
         RowBox[{"ground", "!"}]}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"timeToGround", "=", "maxAirTime"}]}], " ", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "the", " ", "projectile", " ", "never", " ", "hits", " ", "the", " ", 
         "ground"}], ",", " ", 
        RowBox[{
        "so", " ", "we", " ", "set", " ", "plot", " ", "all", " ", "of", " ", 
         "maxAirTime"}]}], "*)"}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "set", " ", "up", " ", "the", " ", "parametric", " ", "plot", " ", 
       "for", " ", "thrust", " ", "stage", " ", "and", " ", "freefall", " ", 
       "stage"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"thrustPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{"pos", ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeThrustApplied"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Red", ",", " ", "Dashed"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"freefallPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{"pos", ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeToGround"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Blue", ",", " ", "Dashed"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "set", " ", "up", " ", "earth", " ", "graphics", " ", "object"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"earthObject", "=", 
      RowBox[{"Graphics", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Darker", "[", "Green", "]"}], ",", 
         RowBox[{"Disk", "[", 
          RowBox[{"centerOfEarth", ",", "radiusEarth"}], "]"}]}], "}"}], 
       "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Show", " ", "the", " ", "final", " ", "plot"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"Show", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"earthObject", ",", "thrustPlot", ",", "freefallPlot"}], 
        "}"}], ",", 
       RowBox[{"Axes", "->", "True"}], ",", 
       RowBox[{"PlotRange", "->", "All"}], ",", 
       RowBox[{"AxesLabel", "->", 
        RowBox[{"{", 
         RowBox[{"\"\<x Position\>\"", ",", "\"\<y Position\>\""}], "}"}]}], 
       ",", 
       RowBox[{"PlotLabel", "->", 
        RowBox[{"StringForm", "[", 
         RowBox[{"\"\<Time to ground: `` seconds\>\"", ",", 
          RowBox[{"timeThrustApplied", "+", "timeToGround"}]}], "]"}]}]}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.8791009568427315`*^9, 3.879100987256689*^9}, {
   3.879101020085478*^9, 3.8791010223973527`*^9}, {3.879101457510738*^9, 
