Cell[BoxData[{
 RowBox[{
  RowBox[{"freefallStage", "[", 
   RowBox[{
   "initialPosx_", ",", "initialPosy_", ",", "initialVelx_", ",", 
    "initialVely_", ",", "mass_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "force", ",", "acc", ",", "vel", ",", "pos", ",", "t", ",", 
      "timeToGround", ",", "pathPlot", ",", "animateTime", ",", 
      "pathAnimation", ",", 
      RowBox[{"g", " ", "=", 
       RowBox[{"-", "9.8"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "set", " ", "up", " ", "the", " ", "equations", " ", "we", " ", "will", 
      " ", 
      RowBox[{"need", ".", " ", "force"}], " ", "and", " ", "acc", " ", "are",
       " ", "constant", " ", "but", " ", "vel", " ", "and", " ", "pos", " ", 
      "are", " ", "functions", " ", "of", " ", "t"}], "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"force", "=", 
      RowBox[{"{", 
       RowBox[{"0", ",", 
        RowBox[{"mass", "*", "g"}]}], "}"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "because", " ", "the", " ", "projectile", " ", "is", " ", "in", " ", 
       "freefall"}], "*)"}], " ", "\[IndentingNewLine]", 
     RowBox[{"acc", "=", 
      RowBox[{"force", "/", "mass"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"vel", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"initialVelx", ",", "initialVely"}], "}"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"acc", ",", "t"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"pos", "=", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"initialPosx", ",", "initialPosy"}], "}"}], "+", 
       RowBox[{"Integrate", "[", 
        RowBox[{"vel", ",", "t"}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "calculate", " ", "the", " ", "time", " ", "it", " ", "will", " ", 
        "take", " ", "the", " ", "projectile", " ", "to", " ", "reach", " ", 
        "the", " ", "ground"}], ",", " ", 
       RowBox[{
       "where", " ", "the", " ", "y", " ", "component", " ", "of", " ", 
        "position", " ", "is", " ", "less", " ", "than", " ", "0"}]}], "*)"}],
      "\[IndentingNewLine]", 
     RowBox[{"timeToGround", "=", 
      RowBox[{"t", "/.", 
       RowBox[{"FindRoot", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"pos", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "==", "0"}], ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "100"}], "}"}]}], "]"}]}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Current", " ", 
        RowBox[{"issue", ":", " ", 
         RowBox[{
         "how", " ", "can", " ", "we", " ", "reliably", " ", "find", " ", 
          "the", " ", "positive", " ", "root"}]}]}], ",", " ", 
       RowBox[{"not", " ", "the", " ", "negative", " ", 
        RowBox[{"one", "?"}]}]}], "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"set", " ", "up", " ", "the", " ", "parametric", " ", "plot"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"pathPlot", "=", 
      RowBox[{"ParametricPlot", "[", 
       RowBox[{"pos", ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "timeToGround"}], "}"}], ",", 
        RowBox[{"PlotStyle", "->", 
         RowBox[{"{", 
          RowBox[{"Red", ",", " ", "Dashed"}], "}"}]}], ",", 
        RowBox[{"AxesLabel", "->", 
         RowBox[{"{", 
          RowBox[{"\"\<x Position\>\"", ",", "\"\<y Position\>\""}], "}"}]}], 
        ",", 
        RowBox[{"PlotLabel", "->", 
         RowBox[{"StringForm", "[", 
          RowBox[{"\"\<Time to ground: `` seconds\>\"", ",", "timeToGround"}],
           "]"}]}]}], "]"}]}]}]}], "\[IndentingNewLine]", 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "We", " ", "could", " ", "try", " ", "animating", " ", "using", " ", 
      "the", " ", "way", " ", "described", " ", "in", " ", "that", " ", 
      "lab"}], ",", " ", 
     RowBox[{
     "by", " ", "creating", " ", "a", " ", "list", " ", "of", " ", "frames", 
      " ", "and", " ", "then", " ", "combining", " ", "them"}], ",", " ", 
     RowBox[{
     "instead", " ", "of", " ", "using", " ", "the", " ", "animate", " ", 
